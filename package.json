{
  "private": true,
  "name": "tuya-panel-kit-docs",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@tuya/dumi-theme-tuya": "1.1.1",
    "@types/qrcode.react": "^1.0.1",
    "antd": "^4.15.2",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.16",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "qrcode.react": "^1.0.1",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "speech-rule-engine": "3.3.1"
  }
}
